<form action="javascript:void(0);" class="form-horizontal" id="loadApp">
  <fieldset>
    <legend>Load an existing Widget</legend>
    <div class="control-group">
      <label class="control-label">Widget Name:</label>
      <div class="controls">
        <select id="selectApp">
          <% _.each(names, function(n) { %>
          <option>
            <%=n%>
          </option>
          <% }); %>
        </select>
      </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <button id="submitButton" class="btn btn-success">Load</button>
      </div>
    </div>
  </fieldset>
</form>